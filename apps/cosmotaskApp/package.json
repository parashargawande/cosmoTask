{
  "name": "cosmotaskapp",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "start": "dotenv -e .env.prod expo start",
    "build:prod": "dotenv -e .env.prod eas build --profile production",
    "dev": "dotenv -e .env.dev expo start",
    "android": "dotenv -e .env.dev expo run:android",
    "ios": "dotenv -e .env.dev expo run:ios",
    "web": "dotenv -e .env.dev expo start --web",
    "deploy": "dotenv -e .env.prod eas build --profile production && eas submit --platform all"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@hookform/resolvers": "^5.0.1",
