{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./",
    "jsx": "react-native",
    "paths": {
      "*": ["*", "*.native", "*.web"]
    },
    "types": ["react-native", "react"]
  }
}
