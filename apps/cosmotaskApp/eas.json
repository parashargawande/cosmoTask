{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_USE_STATIC_BUNDLE": "true",
        "EXPO_NO_CACHE": "true",
        "EXPO_PUBLIC_API_URL": "https://api.cosmotask.app",
        "EXPO_PUBLIC_API_WS_URL": "wss://api.cosmotask.app"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EXPO_USE_STATIC_BUNDLE": "true",
        "EXPO_NO_CACHE": "true",
        "EXPO_PUBLIC_API_URL": "https://api.cosmotask.app",
        "EXPO_PUBLIC_API_WS_URL": "wss://api.cosmotask.app"
      },
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
