{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:cosmotask": "cd apps/cosmotask && eas build -p android --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4"
  },
  "name": "with-vite",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "nohoist": [
    "**/firebase",
    "**/firebase/**"
  ],
  "version": "1.0.0",
  "dependencies": {
    "expo": "~50.0.17",
    "react": "18.2.0",
    "react-native": "0.73.6"
  }
}
